    <webLinks>
        <fullName>USSI_SendAll</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Send All</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)} 
var updatelist = []; //object to update
var records = {!GETRECORDIDS($ObjectType.USSI_Student__c)}; //list of id
var query = &quot;SELECT Id, USSI_Verification_Code__c FROM USSI_Student__c WHERE Id  In &quot; + &quot;(&apos;&quot; + records.toString().replace(/,/g, &quot;&apos;,&apos;&quot;) + &quot;&apos;)&quot;; //query
var result = sforce.connection.query(query); //raw result

var students = result.getArray(&quot;records&quot;); //list of records 
for (var a = 0; a &lt; students.length; a++) {
	var c = new sforce.SObject(&quot;USSI_Student__c&quot;); 
	c.id = students[a].Id; 
	c.USSI_Timestamp__c = new Date();
	var oldCode = students[a].USSI_Verification_Code__c;
	if(!oldCode) c.USSI_Verification_Code__c = Math.floor(Math.random()*89999+10000);
	updatelist.push(c);
}
sforce.connection.update(updatelist);
window.location.reload();</url>
    </webLinks>
